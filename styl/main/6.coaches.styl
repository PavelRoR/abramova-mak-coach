#coaches
	padding 50px 0 200px
	overflow hidden
.coach-img
	position: absolute
	top 0
	right 0
	box-shadow:  0  4px 35px 4px rgba(#020202,.45)
	z-index 3
	width 560px
	height auto
	&-2
		right auto
		left 0
		// height 995px
		// width auto
.coach
	position relative
	&-2
		.coach-name
			text-align: right
	&:first-of-type
		margin-bottom: 100px
	&-name
		font-size: 30px
		font-family: MuB
		color #110d4a
		padding  30px 40px 40px 40px
	&-list
		padding 30px
		border-radius: 35px
		background-color: rgba(#fff,.8)
		position relative
		z-index 9
		width 100%
		max-width 650px
		box-shadow: 0px 4px 37.2px 2.8px rgba(40, 42, 82, 0.56);
		&-2
			float: right
		li
			list-style none
			padding-left: 20px
			position relative
			line-height: 27px
			margin-bottom: 5px
			&:last-child
				margin-bottom: 0
			&:before
				content '-'
				display block
				position: absolute
				left 0
				top 0
			&:last-child
				margin-bottom: 0
	&-sublist
		list-style none
		margin-left: 10px
		li
			&:before
				content none